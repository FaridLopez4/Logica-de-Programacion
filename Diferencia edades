<meta charset="utf-8">
<h2>DIFERECIA DE EDADES ENTRE MI HERMANA Y YO</h2>
<script>
	function imprimir(frase){
		document.write(frase);
		document.write("<br>");
	}
	
	var miEdad=21;
	var edadHermana=50;
	var miNombre="Farid Lopez";
	var nombreHermana="Citlaly Garcia";
	var diferenciaEdad=edadHermana-miEdad;
	imprimir(miNombre+" Tiene: "+miEdad+" años");
	imprimir(nombreHermana+" tiene: "+edadHermana+" años");
	imprimir("La diferencia de edades entre "+miNombre+" y "+nombreHermana+" es de "+diferenciaEdad+" años")
</script>
